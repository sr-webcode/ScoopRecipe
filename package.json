{
  "name": "ScoopRecipeApp",
  "version": "1.0.0",
  "description": "API-based frontend skill test",
  "main": "index.js",
  "scripts": {
    "live": "concurrently \"npm run client\" \"npm run api\" ",
    "client": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
